<!DOCTYPE html>
<html>
<head>
  <title><%= 'Cryptape' %></title>
  <%=	render 'layouts/rails_default' %>
  <%= render 'layouts/shim' %>
</head>
<body>
<%= render 'layouts/header' %>
<div id="id-div-main" class="container">
  <% flash.each do |message_type, message| %>
    <%= content_tag(:div, message, class: "alert alert-#{message_type}") %>
  <% end %>
  <%= yield %>
</div>
</body>
</html>